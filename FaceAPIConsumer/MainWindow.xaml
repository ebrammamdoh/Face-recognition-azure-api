<Window x:Class="FaceAPIConsumer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FaceAPIConsumer"
        mc:Ignorable="d"
        
        Title="MainWindow" Height="350" Width="430">
    <Grid>
        <Image Stretch="UniformToFill" x:Name="FaceImage" HorizontalAlignment="Left"
               Margin="0,0,0,30"></Image>
        <Button x:Name="BtnUplaod" VerticalAlignment="Bottom" Content="Upload Image"
                Height="20" Margin="20,5" Click="BtnUplaod_Click"></Button>
    </Grid>
</Window>
